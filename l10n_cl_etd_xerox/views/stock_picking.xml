<odoo>

    <record model="ir.ui.view" id="view_stock_picking_xerox">
        <field name="name">stock.picking.xerox</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id"
               ref="l10n_cl_etd_stock.view_stock_picking_sii_class"/>
        <field name="arch" type="xml">
            <button name="document_sign" position="attributes">
                <attribute name="context">{'xerox': 52}</attribute>
            </button>
        </field>
    </record>
    
     <record id="view_stock_picking_guia_class" model="ir.ui.view">
        <field name="name">stock.picking.guia.document.class</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form"/>
        <field name="arch" type="xml">
            <field name="partner_id" position="after">
                <field name="move_reason" />
            </field>
        </field>
    </record>

</odoo>
